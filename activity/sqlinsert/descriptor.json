{
  "name": "sqlinsert-activity",
  "type": "flogo:activity",
  "version": "0.0.1",
  "title": "SQL Database Insert Activity",
  "description": "Flogo Activity for SQL",
  "homepage": "https://github.com/TIBCOSoftware/labs-air-contrib/tree/masteractivity/sqlinsert",
  "settings": [
    {
      "name": "dbType",
      "type": "string",
      "allowed": [
        "mysql",
        "oracle",
        "postgres",
        "sqlite",
        "sqlserver"
      ],
      "description": "Database Type",
      "required": false
    },
    {
      "name": "driverName",
      "type": "string",
      "description": "Driver Type",
      "required": false
    },
    {
      "name": "dataSourceName",
      "type": "string",
      "description": "Data Source Name",
      "required": false
    },
    {
      "name": "statement",
      "type": "string",
      "description": "SQL Insert Statement to execute",
      "required": false
    },
    {
      "name": "maxOpenConnections",
      "type": "int",
      "description": "Maximum Open Connections Allowed",
      "required": false
    },
    {
      "name": "maxIdleConnections",
      "type": "int",
      "description": "Maximum Idle Connections Allowed",
      "required": false
    },
    {
      "name": "disablePrepared",
      "type": "bool",
      "description": "Disabel ",
      "required": false
    },
    {
      "name": "labeledResults",
      "type": "bool",
      "description": "Label Results",
      "required": false
    }
  ],
  "input": [
    {
      "name": "params",
      "type": "any",
      "description": "Parameters for query.",
      "required": false
    }
  ],
  "output": [
    {
      "name": "columnNames",
      "type": "any",
      "description": "Column Names ",
      "required": false
    },
    {
      "name": "results",
      "type": "any",
      "description": "Result of SQL Query.",
      "required": false
    }
  ]
}